{
  "name": "ngweb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "docker:build": "docker buildx build --pull --load -t registry.gkny.fr/ephiscience/web .",
    "docker:push": "docker push registry.gkny.fr/ephiscience/web:latest",
    "docker:all": "yarn docker:build && yarn docker:push",
    "docker:run": "docker run --rm -p 0.0.0.0:4000:4000 -it registry.gkny.fr/ephiscience/web",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:fix": "prettier --write .",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "dev:ssr": "ng run ngweb:serve-ssr",
    "serve:ssr": "node dist/ngweb/server/main.js",
    "build:ssr": "ng build && ng run ngweb:server",
    "prerender": "ng run ngweb:prerender"
  },
  "license": "CC-BY-NC-SA",
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.12",
    "@angular/common": "^18.2.12",
    "@angular/compiler": "^18.2.12",
    "@angular/core": "^18.2.12",
    "@angular/forms": "^18.2.12",
    "@angular/platform-browser": "^18.2.12",
    "@angular/platform-browser-dynamic": "^18.2.12",
    "@angular/platform-server": "^18.2.12",
    "@angular/router": "^18.2.12",
    "@angular/service-worker": "^18.2.12",
    "@angular/ssr": "^18.2.12",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "bootstrap": "^4.6.2",
    "core-js": "^3.39.0",
    "express": "^4.21.1",
    "fromentries": "^1.3.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "typeface-arvo": "^1.1.13",
    "typeface-cabin": "^1.1.13",
    "typeface-cabin-condensed": "^1.1.13",
    "typeface-roboto": "^1.1.13",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-devkit/core": "^18.2.12",
    "@angular-devkit/schematics": "^18.2.12",
    "@angular-eslint/builder": "^18.4.2",
    "@angular-eslint/eslint-plugin": "^18.4.2",
    "@angular-eslint/eslint-plugin-template": "^18.4.2",
    "@angular-eslint/schematics": "18.4.2",
    "@angular-eslint/template-parser": "^18.4.2",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.12",
    "@angular/language-service": "^18.2.12",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~3.6.11",
    "@types/jasminewd2": "~2.0.13",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.9.4",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@typescript-eslint/types": "^8.16.0",
    "@typescript-eslint/utils": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.5.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.4.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "protractor": "~7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.4"
  },
  "prettier": {
    "printWidth": 140,
    "bracketSpacing": true,
    "tabWidth": 2,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "semi": true,
    "singleQuote": true
  },
  "volta": {
    "node": "20.18.1",
    "yarn": "1.22.22"
  }
}
