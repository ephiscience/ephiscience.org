{
  "name": "ngweb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "docker:build": "docker build --pull -t registry.gkny.fr/ephiscience/web .",
    "docker:push": "docker build --pull -t registry.gkny.fr/ephiscience/web . && docker push registry.gkny.fr/ephiscience/web:latest",
    "docker:run": "docker run --rm -p 0.0.0.0:2015:2015 -it registry.gkny.fr/ephiscience/web",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\""
  },
  "license": "CC-BY-NC-SA",
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@angular/service-worker": "~9.0.1",
    "@ngx-translate/core": "^12.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.4",
    "fromentries": "^1.1.0",
    "hammerjs": "^2.0.8",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "typeface-arvo": "^0.0.72",
    "typeface-cabin": "^0.0.72",
    "typeface-cabin-condensed": "^0.0.72",
    "typeface-roboto": "^0.0.75",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@ngrx/schematics": "^8.6.0",
    "@types/jasmine": "^3.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^3.0.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "protractor": "~5.4.0",
    "ts-node": "^8.0.0",
    "tslint": "^5.12.0",
    "typescript": "3.7.5"
  },
  "prettier": {
    "printWidth": 140,
    "bracketSpacing": true,
    "tabWidth": 2,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "semi": false,
    "singleQuote": true
  }
}
